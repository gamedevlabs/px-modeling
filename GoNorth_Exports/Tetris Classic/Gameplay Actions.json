  "Text": [
    {
      "Text": "The player places a tetromino.",
      "Id": "aa01b247-0925-43cf-9e80-4704700ca3bc",
      "X": 274,
      "Y": 298
    },
    {
      "Text": "The player tops out, the game stops.",
      "Id": "862be2c7-a7b4-42ce-a5a7-e21276719dd5",
      "X": 676,
      "Y": 87
    },
    {
      "Text": "The player neither tops out nor finishes a line.",
      "Id": "c8b10b10-ca50-40bb-81ee-5cfaf8026e6f",
      "X": 672,
      "Y": 273
    },
    {
      "Text": "The player finishes at least one line.",
      "Id": "e994b805-b16f-4017-8979-814ee37857d7",
      "X": 682,
      "Y": 484
    },
    {
      "Text": "The fill level stays the same.",
      "Id": "4edfb3c4-b892-4235-8cce-c0e15f487e1d",
      "X": 1108,
      "Y": 320
    },
    {
      "Text": "The fill level increases by one stage.",
      "Id": "a216b5a3-f14c-4a27-a625-97ac477e35ab",
      "X": 1104,
      "Y": 107
    },
    {
      "Text": "The fill level decreases by one stage.",
      "Id": "a8f6ce0d-5126-422b-ae11-e33803fdbee7",
      "X": 1123,
      "Y": 521
    },
    {
      "Text": "The fill level decreases by two stages.",
      "Id": "d646c5e9-18d2-4bae-aff1-ecf94837173d",
      "X": 1127,
      "Y": 733
    }
  ],
